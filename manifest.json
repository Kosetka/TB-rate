{
	"manifest_version": 2,
	"name": "TeamBox Alert",
	"version": "1.0",
	"content_scripts": [
		{
			"matches": [
				"http://teambox.pl/*",
				"http://www.teambox.pl/*"
			],
			"js": [
				"content.js"
			]
		}
	],
	"permissions": [
		"storage"
	],
	"web_accessible_resources": [
		"style.css"
	],
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_icon": "t.png",
		"default_title": "TeamBox Alert",
		"default_popup": "index.html"
	}
}
